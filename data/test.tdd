test test_fT_ME :
    scope :
        filepath : '/pfunit_example'
        modules :
            cfo_example
    var :
        dp : int, PARAMETER = KIND( 0D0 )
        temperature : REAL( dp ) = 20D0
        expected : REAL( dp ) = (1D0 + (temperature - 15D0)*9.5D0)/260D0
    assert Equal (
        in :
            expected , k g # 3 kg input
        out :
            fT_ME( temperature ) , k g # 9.0 kg output
        tolerance : 1D-12
        failmessage : 'custom assert fail msg'
        # comment for assertion
        )